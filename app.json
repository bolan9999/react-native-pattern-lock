{
  "expo": {
    "name": "react-native-pattern-lock",
    "slug": "react-native-pattern-lock",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.bolan9999.PatternLock"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A smooth pattern lock component for react native.\n\n",
    "githubUrl": "https://github.com/bolan9999/react-native-pattern-lock"
  }
}
